{
  "Polygon": {
    "WebSocketUrl": "wss://delayed.polygon.io/stocks",
    "ApiKey": "__POLYGON_API_KEY__",
    "Symbols": "AM.TSLA,AM.RZLV,AM.PATH"
  },
  "Database": {
    "ConnectionString": "__DB_CONNECTION_STRING__"
  },
  "Ingestion": {
    "TimeframeIdMinute": 1,
    "BatchSize": 500,
    "FlushSeconds": 1,
    "ReconnectBaseDelayMs": 1000,
    "ReconnectMaxDelayMs": 30000
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [ { "Name": "Console" } ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
